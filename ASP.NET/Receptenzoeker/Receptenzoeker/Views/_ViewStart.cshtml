@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    @if (HttpContextAccessor.HttpContext.Session.Get("Admin") != null)
    {
        Layout = "Dashboard";
    }
    else if (HttpContextAccessor.HttpContext.Session.Get("User") != null)
    {
        Layout = "_Layout";
    }
    else
        Layout = "_Layout";
 }